<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 64 64" width="64" height="64" role="img" aria-label="Cel: masa">
  <defs>
    <!-- gradient obrysu -->
    <linearGradient id="gradStroke" x1="0" y1="0" x2="1" y2="1">
      <stop offset="0%" stop-color="#14d39a"/>
      <stop offset="100%" stop-color="#22d3ee"/>
    </linearGradient>
    <!-- gradient wypełnienia hantli -->
    <linearGradient id="gradFill" x1="0" y1="0" x2="1" y2="1">
      <stop offset="0%" stop-color="#f5f5f5"/>
      <stop offset="50%" stop-color="#ffffff"/>
      <stop offset="100%" stop-color="#d4d4d4"/>
    </linearGradient>
    <!-- cień pod hantlą -->
    <radialGradient id="shadowGrad" cx="0.5" cy="0.5" r="0.5">
      <stop offset="0%" stop-color="#000" stop-opacity="0.25"/>
      <stop offset="100%" stop-color="#000" stop-opacity="0"/>
    </radialGradient>
  </defs>

  <!-- cień pod hantlą -->
  <ellipse cx="32" cy="52" rx="18" ry="4" fill="url(#shadowGrad)" />

  <!-- hantle -->
  <!-- lewy ciężar -->
  <rect x="10" y="20" width="8" height="24" rx="2" fill="url(#gradFill)" stroke="url(#gradStroke)" stroke-width="2"/>
  <rect x="6" y="24" width="6" height="16" rx="2" fill="url(#gradFill)" stroke="url(#gradStroke)" stroke-width="1.5"/>

  <!-- prawy ciężar -->
  <rect x="46" y="20" width="8" height="24" rx="2" fill="url(#gradFill)" stroke="url(#gradStroke)" stroke-width="2"/>
  <rect x="52" y="24" width="6" height="16" rx="2" fill="url(#gradFill)" stroke="url(#gradStroke)" stroke-width="1.5"/>

  <!-- uchwyt -->
  <rect x="18" y="26" width="28" height="12" rx="3" fill="url(#gradFill)" stroke="url(#gradStroke)" stroke-width="2"/>

  <!-- połysk na uchwycie -->
  <rect x="18" y="26" width="28" height="6" rx="3" fill="#fff" opacity="0.35"/>
</svg>